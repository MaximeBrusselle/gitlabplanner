		"@clerk/astro": "^1.5.1",
		"@libsql/client": "^0.14.0",
		"@nanostores/react": "^0.8.2",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"astro": "^4.16.16",
		"better-sqlite3": "^11.6.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.6",
		"drizzle-orm": "^0.36.4",
		"lucide-react": "^0.468.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"recharts": "^2.14.1",
		"tailwind-merge": "^2.5.5",
		"tailwindcss": "^3.4.15",
